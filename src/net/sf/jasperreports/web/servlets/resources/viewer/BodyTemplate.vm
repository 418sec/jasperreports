<div class='jrcontainer' style='display: table-cell'></div>
<script class='jasperreports'>
    var reportPromise = null,
        viewer = null;
    
    require(["jasperreports-viewer"], function(Viewer) {
        viewer = new Viewer({
            at: "jrcontainer",
            reporturi: "${reportUri}",
            async: ${async},
            page: "${page}"
        });

        reportPromise = viewer.loadReport();
    });
    
</script>