<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
       http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
       http://www.springframework.org/schema/util 
       http://www.springframework.org/schema/util/spring-util-2.5.xsd">

	<!-- general constants -->
	<util:constant id="isTrue" static-field="java.lang.Boolean.TRUE"/>
	<util:constant id="isFalse" static-field="java.lang.Boolean.FALSE"/>

	<util:constant id="fontPlainStyle" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.FONT_PLAIN_STYLE"/>
	<util:constant id="fontBoldStyle" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.FONT_BOLD_STYLE"/>
	<util:constant id="fontItalicStyle" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.FONT_ITALIC_STYLE"/>

	<util:constant id="transparentPaint" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.TRANSPARENT_PAINT"/>

	<util:constant id="whiteColor" static-field="java.awt.Color.WHITE"/>
	<util:constant id="blackColor" static-field="java.awt.Color.BLACK"/>
	<util:constant id="greyColor" static-field="java.awt.Color.GRAY"/>
	<util:constant id="darkGrayColor" static-field="java.awt.Color.DARK_GRAY"/>
	<util:constant id="lightGrayColor" static-field="java.awt.Color.LIGHT_GRAY"/>

	<util:constant id="eyeCandySixtiesSeriesColors" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.EYE_CANDY_SIXTIES_COLORS"/>
	<util:constant id="eyeCandySixtiesSeriesGradientPaints" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.EYE_CANDY_SIXTIES_GRADIENT_PAINTS"/>

	<util:constant id="rectangleEdgeTop" static-field="org.jfree.ui.RectangleEdge.TOP"/>
	<util:constant id="rectangleEdgeLeft" static-field="org.jfree.ui.RectangleEdge.LEFT"/>
	<util:constant id="rectangleEdgeBottom" static-field="org.jfree.ui.RectangleEdge.BOTTOM"/>
	<util:constant id="rectangleEdgeRight" static-field="org.jfree.ui.RectangleEdge.RIGHT"/>
	
	<util:constant id="centerHorizontalAlignment" static-field="org.jfree.ui.HorizontalAlignment.CENTER"/>
	<util:constant id="leftHorizontalAlignment" static-field="org.jfree.ui.HorizontalAlignment.LEFT"/>
	<util:constant id="rightHorizontalAlignment" static-field="org.jfree.ui.HorizontalAlignment.RIGHT"/>
	
	<util:constant id="blockBorderNone" static-field="org.jfree.chart.block.BlockBorder.NONE"/>
	
	<util:constant id="unitTypeAbsolute" static-field="org.jfree.util.UnitType.ABSOLUTE"/>
	<util:constant id="unitTypeRelative" static-field="org.jfree.util.UnitType.RELATIVE"/>
	
	<util:constant id="plotOrientationHorizontal" static-field="org.jfree.chart.plot.PlotOrientation.HORIZONTAL"/>
	<util:constant id="plotOrientationVertical" static-field="org.jfree.chart.plot.PlotOrientation.VERTICAL"/>

	<bean id="halfPointStroke" class="java.awt.BasicStroke">
		<constructor-arg index="0" value="0.5f"/>
	</bean>

	<bean id="onePointStroke" class="java.awt.BasicStroke">
		<constructor-arg index="0" value="1f"/>
	</bean>

	<bean id="twoPointStroke" class="java.awt.BasicStroke">
		<constructor-arg index="0" value="2f"/>
	</bean>

	<bean id="threePointStroke" class="java.awt.BasicStroke">
		<constructor-arg index="0" value="3f"/>
	</bean>

	<!-- chart property names -->
	
	<util:constant id="defaultBackgroundPaint" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_BACKGROUND_PAINT"/>
	<util:constant id="defaultBaseFontSize" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_BASEFONT_SIZE"/>
	
	<util:constant id="defaultTitlePosition" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_TITLE_POSITION"/>
	<util:constant id="defaultTitleBaseFontBoldStyle" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_TITLE_BASEFONT_BOLD_STYLE"/>
	<util:constant id="defaultTitleBaseFontItalicStyle" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_TITLE_BASEFONT_ITALIC_STYLE"/>
	<util:constant id="defaultTitleForecolor" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_TITLE_FORECOLOR"/>
	<util:constant id="defaultTitleBackcolor" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_TITLE_BACKCOLOR"/>
	<util:constant id="defaultTitleBaseFontSize" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_TITLE_BASEFONT_SIZE"/>
	<util:constant id="defaultTitleHorizontalAlignment" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_TITLE_HORIZONTAL_ALIGNMENT"/>

	<util:constant id="defaultSubtitlePosition" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_SUBTITLE_POSITION"/>
	<util:constant id="defaultSubtitleBaseFontBoldStyle" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_SUBTITLE_BASEFONT_BOLD_STYLE"/>
	<util:constant id="defaultSubtitleBaseFontItalicStyle" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_SUBTITLE_BASEFONT_ITALIC_STYLE"/>
	<util:constant id="defaultSubtitleForecolor" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_SUBTITLE_FORECOLOR"/>
	<util:constant id="defaultSubtitleBackcolor" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_SUBTITLE_BACKCOLOR"/>
	<util:constant id="defaultSubtitleBaseFontSize" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_SUBTITLE_BASEFONT_SIZE"/>
	<util:constant id="defaultSubtitleHorizontalAlignment" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_SUBTITLE_HORIZONTAL_ALIGNMENT"/>

	<util:constant id="defaultLegendPosition" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_LEGEND_POSITION"/>
	<util:constant id="defaultLegendBaseFontBoldStyle" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_LEGEND_BASEFONT_BOLD_STYLE"/>
	<util:constant id="defaultLegendBaseFontItalicStyle" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_LEGEND_BASEFONT_ITALIC_STYLE"/>
	<util:constant id="defaultLegendForecolor" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_LEGEND_FORECOLOR"/>
	<util:constant id="defaultLegendBackcolor" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_LEGEND_BACKCOLOR"/>
	<util:constant id="defaultLegendBaseFontSize" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_LEGEND_BASEFONT_SIZE"/>
	<util:constant id="defaultLegendHorizontalAlignment" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_LEGEND_HORIZONTAL_ALIGNMENT"/>
	<util:constant id="defaultLegendBlockFrame" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_LEGEND_BLOCK_FRAME"/>

	<util:constant id="defaultSeriesColors" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_SERIES_COLORS"/>
	<util:constant id="defaultSeriesGradientPaints" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_SERIES_GRADIENT_PAINTS"/>
	<util:constant id="defaultChartBorderPaint" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_CHART_BORDER_PAINT"/>
	<util:constant id="defaultChartBorderStroke" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_CHART_BORDER_STROKE"/>
	<util:constant id="defaultChartBorderVisible" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_CHART_BORDER_VISIBLE"/>
	<util:constant id="defaultChartAntiAlias" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_CHART_ANTI_ALIAS"/>
	<util:constant id="defaultChartPadding" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_CHART_PADDING"/>
	<util:constant id="defaultUnitType" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_UNIT_TYPE"/>

	<!-- plot property names -->
	
	<util:constant id="defaultPlotBackgroundAlpha" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_PLOT_BACKGROUND_ALPHA"/>
	<util:constant id="defaultPlotForegroundAlpha" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_PLOT_FOREGROUND_ALPHA"/>
	<util:constant id="defaultPlotLabelRotation" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_PLOT_LABEL_ROTATION"/>
	<util:constant id="defaultPlotOrientation" static-field="net.sf.jasperreports.charts.themes.ChartThemesConstants.DEFAULT_PLOT_ORIENTATION"/>

</beans>
