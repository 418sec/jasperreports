<ivy-module version="2.0">
	<info organisation="net.sf.jasperreports" module="jasperreports">
		<extends organisation="net.sf.jasperreports" module="jasperreports" revision="*" location="../../../ivy.xml"/>
	</info>
	<dependencies>
		<dependency org="org.hsqldb" name="hsqldb" rev="2.4.0" conf="test->*"/>
		<dependency org="commons-codec" name="commons-codec" rev="1.5" conf="test->*"/>
		<dependency org="org.apache.xmlgraphics" name="batik-ext" rev="1.9" conf="test->*"/>
		<dependency org="org.apache.xmlgraphics" name="batik-parser" rev="1.9" conf="test->*"/>
		<dependency org="org.apache.xmlgraphics" name="batik-script" rev="1.9" conf="test->*"/>
		<dependency org="org.apache.xmlgraphics" name="batik-xml" rev="1.9" conf="test->*"/>
		<dependency org="org.apache.xmlgraphics" name="xmlgraphics-commons" rev="2.0" conf="test->*"/>

		<!-- Dependencies required to compile a CVC from sources -->
		<dependency org="com.google.javascript" name="closure-compiler" rev="v20160208"/>

	</dependencies>
</ivy-module>
