<?xml version="1.0" encoding="UTF-8"?>

<jasperReport
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="http://jasperreports.sourceforge.net/jasperreports"
		xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd
				http://jasperreports.sourceforge.net/jasperreports/spiderchart http://jasperreports.sourceforge.net/dtds/spiderchart.xsd"
		name="SpiderChart">
	
	<import value="net.sf.jasperreports.engine.data.JRCsvDataSource"/>
	
	<subDataset name="categoryDataset">
		<field name="value" class="java.lang.Double">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="series" class="java.lang.String">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="category" class="java.lang.String">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<group name="category">
			<groupExpression><![CDATA[$F{category}]]></groupExpression>
		</group>
	</subDataset>
	
	<parameter name="spiderDatasource" class="net.sf.jasperreports.engine.data.JRCsvDataSource"/>
	
	<title>
		<band height="370">
			<line>
				<reportElement x="0" y="0" width="515" height="1"/>
				<graphicElement/>
			</line>
			<line>
				<reportElement x="0" y="3" width="515" height="1"/>
				<graphicElement/>
			</line>
			<staticText>
				<reportElement x="0" y="10" width="515" height="30"/>
				<textElement>
					<font size="22"/>
				</textElement>
				<text><![CDATA[Spider Chart Component Report]]></text>
			</staticText>
			<textField hyperlinkType="Reference">
				<reportElement x="0" y="40" width="515" height="20"/>
				<textElement/>
				<textFieldExpression class="java.lang.String"><![CDATA["http://www.jfree.org/jfreechart"]]></textFieldExpression>
				<hyperlinkReferenceExpression><![CDATA["http://www.jfree.org/jfreechart"]]></hyperlinkReferenceExpression>
			</textField>
			<line>
				<reportElement x="0" y="60" width="515" height="1"/>
				<graphicElement/>
			</line>
			<line>
				<reportElement x="0" y="63" width="515" height="1"/>
				<graphicElement/>
			</line>
			<componentElement>
				<reportElement x="0" y="70" width="500" height="300"/>
				<sc:spiderChart xmlns:sc="http://jasperreports.sourceforge.net/jasperreports/spiderchart" evaluationTime="Report">
					<sc:chartSettings isShowLegend="true" >
						<chartTitle>
							<titleExpression><![CDATA["Spider Chart"]]></titleExpression>
						</chartTitle>
						<chartSubtitle>
							<subtitleExpression><![CDATA["Chart Displaying Spider Webs"]]></subtitleExpression>
						</chartSubtitle>
						<chartLegend/>
						<hyperlinkTooltipExpression><![CDATA["Spider Chart"]]></hyperlinkTooltipExpression>
					</sc:chartSettings>
					<sc:spiderDataset>
						<dataset>
							<datasetRun subDataset="categoryDataset">
								<dataSourceExpression><![CDATA[$P{spiderDatasource}]]></dataSourceExpression>
							</datasetRun>
						</dataset>
						<categorySeries>
							<seriesExpression><![CDATA[$F{series}]]></seriesExpression>
							<categoryExpression><![CDATA[$F{category}]]></categoryExpression>
							<valueExpression><![CDATA[$V{value}]]></valueExpression>
						</categorySeries>
					</sc:spiderDataset>
					<sc:spiderPlot rotation="Clockwise" startAngle="45" headPercent="0.1" interiorGap="0.3" axisLineColor="#88ee11" backcolor="yellow">
						<labelFont>
							<font isBold="true"/>
						</labelFont>
						<sc:maxValueExpression><![CDATA[Double.valueOf(10)]]></sc:maxValueExpression>
					</sc:spiderPlot>
				</sc:spiderChart>
			</componentElement>
		</band>
	</title>

</jasperReport>