<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
       http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
       http://www.springframework.org/schema/util 
       http://www.springframework.org/schema/util/spring-util-2.5.xsd">

	<util:map id="defaultPlotProperties">
   	    <entry key-ref="defaultPlotBackgroundAlpha">
   	    	<value type="java.lang.Float">1f</value>
   	    </entry>
   	    <entry key-ref="defaultPlotForegroundAlpha">
   	    	<value type="java.lang.Float">1f</value>
   	    </entry>
   	    <entry key-ref="defaultPlotLabelRotation">
   	    	<value type="java.lang.Double">0.0</value>
   	    </entry>
   	    <entry key-ref="defaultPlotOrientation" value-ref="plotOrientationVertical"/>
	</util:map>

	<import resource="chartConstantsBean.xml"/>
</beans>
